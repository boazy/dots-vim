{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-underscore-dangle": 0,
      "strict": 0,
      "quotes": 0,
      "semi": 0
    }
}
